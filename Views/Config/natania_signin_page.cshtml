<script>

    function sign_in() {
        var data = {
            'email': $('#email').val(),
            'password': $('#password').val()
        };
        $.ajax({
            type: 'post',
            url: 'sign_in',
            data: data,
            async: true,
            success: function (response) {
                var result = JSON.parse(response);
                if (result[0].result == "success") {
                    swal({
                        title: 'Login Success!',
                        text: 'Redirecting you to landing page',
                        timer: 2000,
                        icon: "success",
                        button: false
                    }).then(
                        function () {
                            get_details(result[0].id);
                            clear_fields();
                            
                        }
                    )
                }
                else if (result[0].result == "error-not-verified") {

                    swal({
                        title: 'Error loging In',
                        text: 'Your Email is not Verified',
                        timer: 3000,
                        icon: "error",
                        button: false
                    }).then(
                        function () {
                            clear_fields();
                        }
                    )
                }
                else if (result[0].result == "error-invalid-credentials") {
                    swal({
                        title: 'Error loging In',
                        text: 'Email or Password is Incorrect',
                        timer: 3000,
                        icon: "error",
                        button: false
                    }).then(
                        function () {
                            clear_fields();
                        }
                    )
                }
                else {
                    swal({
                        title: 'Error loging In',
                        text: 'Server Error, Try Again Later',
                        timer: 3000,
                        icon: "error",
                        button: false
                    }).then(
                        function () {
                            clear_fields();
                        }
                    )
                }
            }
        });
    }
    function get_details() {
        $.ajax({
            type: 'get',
            url: 'get_details',
            async: true,
            success: function (response) {
                window.location.href = 'natania_landing_page'
            }
        })
    }

    function clear_fields() {
        $('#email').val('');
        $('#password').val('');
    }


</script>
