<script>
    get_admin_details();

    function get_admin_details() {
        var data = '';
        $.ajax({
            type: 'post',
            url: 'Admin/get_admin_details',
            data: data,
            async: true,
            success: function (response) {
                var result = JSON.parse(response);
                var tableBody = document.getElementById("homeowners_list").getElementsByTagName('tbody')[0];
                tableBody.innerHTML = '';
                var count = 1;
                result.forEach(function (item) {
                    var row = tableBody.insertRow();
                    row.insertCell(0).innerHTML = count;
                    row.insertCell(1).innerHTML += item.phase;
                    row.insertCell(2).innerHTML += item.block;
                    row.insertCell(3).innerHTML += item.lot;
                    row.insertCell(4).innerHTML += item.street;
                    row.insertCell(5).innerHTML += item.last_name;
                    row.insertCell(6).innerHTML += item.first_name;
                    row.insertCell(7).innerHTML += `<button type="button" class="btn btn-primary" data-toggle="modal" data-target="#ViewDetails" onclick="view_details(`+item.acc_id+`)">
                                                    <span class="flaticon-visible svg-icon svg-icon-lg"></span>View Details</button>`;
                    count++;
                });
            }
        })
    }

    function view_details(id) {
        get_homeowners_details(id);
        get_homeowners_family(id);
        get_homeowners_pets(id);
        get_homeowners_vehicles(id);
    }

    function get_homeowners_details(id) {
        var data = {
            acc_id: id
        };
        $.ajax({
            type: 'post',
            url: 'Admin/get_homeowners_details',
            data: data,
            async: true,
            success: function (response) {
                var result = JSON.parse(response);
                document.getElementById("name").innerHTML = result[0].first_name + ' ' + result[0].last_name;
                document.getElementById("gender").innerHTML = result[0].gender;
                document.getElementById("age").innerHTML = result[0].age;
                document.getElementById("phone_no").innerHTML = result[0].phone_no;
                document.getElementById("email").innerHTML = result[0].email;

                document.getElementById("address1").innerHTML = 'Phase ' + result[0].phase + ' Block ' + result[0].block + ' lot ' + result[0].lot + ' ' + result[0].street + ' Street, Barangay ' + result[0].barangay + ',';
                document.getElementById("address2").innerHTML = result[0].city + ' City, ' + result[0].province + ', ' + result[0].country + ', ' + result[0].postal_code;
            }
        });
    }

    function get_homeowners_family(id) {
        var data = {
            acc_id: id
        };
        $.ajax({
            type: 'post',
            url: 'Admin/get_homeowners_family',
            data: data,
            async: true,
            success: function (response) {
                var result = JSON.parse(response);
                var tableBody = document.getElementById("family_list").getElementsByTagName('tbody')[0];
                tableBody.innerHTML = '';
                var count = 1;
                result.forEach(function (item) {
                    var row = tableBody.insertRow();
                    row.insertCell(0).innerHTML = count;
                    row.insertCell(1).innerHTML += item.first_name;
                    row.insertCell(2).innerHTML += item.last_name;
                    row.insertCell(3).innerHTML += item.gender;
                    row.insertCell(4).innerHTML += item.age;
                    count++;
                });
            }
        });
    }

    function get_homeowners_pets(id) {
        var data = {
            acc_id: id
        };
        $.ajax({
            type: 'post',
            url: 'Admin/get_homeowners_pets',
            data: data,
            async: true,
            success: function (response) {
                var result = JSON.parse(response);
                var tableBody = document.getElementById("pets_list").getElementsByTagName('tbody')[0];
                tableBody.innerHTML = '';
                var count = 1;
                result.forEach(function (item) {
                    var row = tableBody.insertRow();
                    row.insertCell(0).innerHTML = count;
                    row.insertCell(1).innerHTML += item.name;
                    row.insertCell(2).innerHTML += item.type;
                    row.insertCell(3).innerHTML += item.breed;
                    row.insertCell(4).innerHTML += item.gender;
                    if (item.is_vaccinated == 1) row.insertCell(5).innerHTML += 'Yes';
                    else row.insertCell(5).innerHTML += 'No';
                    count++;
                });
            }
        });
    }

    function get_homeowners_vehicles(id) {
        var data = {
            acc_id: id
        };
        $.ajax({
            type: 'post',
            url: 'Admin/get_homeowners_vehicles',
            data: data,
            async: true,
            success: function (response) {
                var result = JSON.parse(response);
                var tableBody = document.getElementById("vehicles_list").getElementsByTagName('tbody')[0];
                tableBody.innerHTML = '';
                var count = 1;
                result.forEach(function (item) {
                    var row = tableBody.insertRow();
                    row.insertCell(0).innerHTML = count;
                    row.insertCell(1).innerHTML += item.type;
                    row.insertCell(2).innerHTML += item.brand;
                    row.insertCell(3).innerHTML += item.model;
                    row.insertCell(4).innerHTML += item.plate_no;
                    if (item.status == 1) row.insertCell(5).innerHTML += `<span class="label label-inline label-light-success font-weight-bold">Approved</span>`;
                    else row.insertCell(5).innerHTML += `<span class="label label-inline label-light-warning font-weight-bold">Pending</span>`
                    count++;
                });
            }
        });
    }
</script>

