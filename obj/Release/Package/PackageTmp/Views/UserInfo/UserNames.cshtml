@model MHD.Models.UserInfoModel

@{
    ViewBag.Title = "UserNames";
    Layout = "~/Views/Shared/_Layout_Receptionist.cshtml";
}
<div id="content" class="content">

    <!-- begin panel -->

    <div class="panel panel-inverse">
        <div class="panel-heading">
            <h4 class="panel-title">Packages</h4>
        </div>
        <div class="panel-body">
            @ViewData["firstname"]
            <h2>UserNames</h2>
            Get User Names PAge
            @foreach (var data in Model.UserInfo)
            {
                <td class="text-center">Firstname @data.firstname</td>;

                <td class="text-center">Lastname=@data.lastname</td>;
                <br/>
            }
        </div>
    </div>
</div>

